<%- include ('partial/header.ejs') %>

	
		<div class=" homeHeadContainer2" data-aos="fade-down-right">
			<div class="jumbotron jumbotronTitle jumbotronHead">
				<h1 class="display-4" data-aos="fade-right">Contact Us</h1>
				<p class="lead" data-aos="fade-right" data-aos-anchor="#trigger-left" data-aos-anchor-placement="top-bottom">Contact For Premium Business Services</p>
			</div>
		</div>
	</div>	
		<div class="container containerText firstheaddiv" id="trigger-left">
			<div class="jumbotron jumbotronText">
				<em><h4 class="display-5" data-aos="fade-up" data-aos-anchor="#trigger-fadeup" data-aos-anchor-placement="top-bottom">We are a ping away</h4></em>
			</div>
		</div>

		<div class="hideDiv" >
			<div class="contactRow" id="trigger-fadeup">
				<div class="row contactform">
					<div class="col-12 col-lg-7 contact-column">
						<form  method="post" action="/send">
							<h5>Hi there, Drop us a line! We would be happy to hear from you</h5>
							<div>
								<% if(success && success.length>0){ %>
										<div class="alert alert-success">
											<%= success %>
										</div>
								<%	} else if(error && error.length>0){ %> %>
								
										<div class="alert alert-danger">
											<%= error %>
										</div>
								<%	} %>
							</div>
							<br>
							<div class="form-group">
		                        <label for="name">Name</label>
		                        <input type="text" class="form-control" id="name" name="name">
		                    </div>
		                    <div class="form-group">
		                        <label for="email">Email</label>
		                        <input type="email" class="form-control" id="email" name="email">
		                    </div>
		                    <div class="form-group">
		                        <label for="enquiry">Enquiry</label>
		                        <textarea class="form-control" id="enquiry" name="enquiry" rows="4"></textarea>
		                    </div>
		                    <input type="submit" class="btn btn-primary" name="submit" value="submit">
		                    <input type="reset" class="btn btn-primary" name="reset" value="reset">
		                    
	                    </form>
	                </div>
	                <div class="col-12 col-lg-5 contact-column">
	                	<ul>
	                		<li class="icons-list">
				                <strong>Address:</strong><br>
				                Surrendra Nagar, Adambakkam<br>
				                Chennai - 600 088, India<br>
				            </li>
				            <br>
			                <li>
